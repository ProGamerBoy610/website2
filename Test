    <script>
        function decodeOctal(str) {
            let result = '';
            let i = 0;
            while (i < str.length) {
                if (str[i] === '\\' && i + 3 < str.length) {
                    const octal = str.substring(i + 1, i + 4);
                    if (/^\d{3}$/.test(octal)) {
                        result += String.fromCharCode(parseInt(octal, 8));
                        i += 4;
                        continue;
                    }
                }
                result += str[i];
                i++;
            }
            return result;
        }

        function decodeBase64(str) {
            try {
                return atob(str);
            } catch (e) {
                return null;
            }
        }

        function deobfuscate() {
            const input = document.getElementById('input').value;
            const output = document.getElementById('output');
            const stats = document.getElementById('stats');
            
            if (!input.trim()) {
                output.textContent = 'Please paste obfuscated code first!';
                return;
            }
            
            let result = '=== DEOBFUSCATION RESULTS ===\n\n';
            let stringCount = 0;
            let base64Count = 0;
            const decodedStrings = [];
            const base64Decoded = [];
            
            // Extract all quoted strings with octal sequences
            const stringPattern = /"([^"\\]*(?:\\[0-9]{3}[^"\\]*)*)"/g;
            let match;
            
            while ((match = stringPattern.exec(input)) !== null) {
                const encoded = match[1];
                const decoded = decodeOctal(encoded);
                stringCount++;
                decodedStrings.push(decoded);
                
                result += `[${stringCount}] Original: "${encoded}"\n`;
                result += `    Decoded:  "${decoded}"\n`;
                
                // Try base64 decode
                if (decoded.includes('=') || decoded.length % 4 === 0) {
                    const b64 = decodeBase64(decoded);
                    if (b64 && /^[\x20-\x7E\s]*$/.test(b64)) {
                        result += `    Base64:   "${b64}"\n`;
                        base64Count++;
                        base64Decoded.push(b64);
                    }
                }
                result += '\n';
            }
            
            // Check for obfuscator info
            const versionMatch = input.match(/v(\d+\.\d+\.\d+)/);
            const urlMatch = input.match(/https?:\/\/[^\s\]]+/);
            
            output.textContent = result;
            
            // Update statistics
            stats.innerHTML = `
                <h3>üìä Statistics</h3>
                <div class="stat-item"><strong>Obfuscator:</strong> ${urlMatch ? urlMatch[0] : 'Unknown'}</div>
                <div class="stat-item"><strong>Version:</strong> ${versionMatch ? versionMatch[1] : 'Unknown'}</div>
                <div class="stat-item"><strong>Total Strings Found:</strong> ${stringCount}</div>
                <div class="stat-item"><strong>Base64 Decoded:</strong> ${base64Count}</div>
                <div class="stat-item"><strong>Input Size:</strong> ${input.length} characters</div>
                <div class="stat-item"><strong>Output Size:</strong> ${result.length} characters</div>
            `;
            
            if (stringCount === 0) {
                output.textContent = '‚ö†Ô∏è No encoded strings found. Make sure you pasted valid obfuscated Lua code.';
            }
        }

        function clearAll() {
            document.getElementById('input').value = '';
            document.getElementById('output').textContent = 'Results will appear here...';
            document.getElementById('stats').innerHTML = `
                <h3>Statistics</h3>
                <div class="stat-item">Click "Deobfuscate" to analyze the code</div>
            `;
        }

        function copyOutput() {
            const output = document.getElementById('output').textContent;
            navigator.clipboard.writeText(output).then(() => {
                alert('‚úì Output copied to clipboard!');
            });
        }

        // Allow Ctrl+Enter to deobfuscate
        document.getElementById('input').addEventListener('keydown', (e) => {
            if (e.ctrlKey && e.key === 'Enter') {
                deobfuscate();
            }
        });
    </script>
